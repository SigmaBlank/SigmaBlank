                    android:layout_height="@dimen/_48pxh"
                    android:layout_marginBottom="@dimen/_9pxv"
                    android:layout_marginTop="@dimen/_11pxv"
                    android:layout_width="@dimen/_25pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_arrowleft"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_arrowleft"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtTableReserve"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_33pxh"
                    android:layout_weight="0.92"
                    android:layout_width="0dp" android:singleLine="true"
                    android:text="@{tableReserveLunchVM.tableReserveLunchModel.txtTableReserve}"
                    style="@style/txtIstokWebbold48" tools:text="@string/lbl_table_reserve"/>
            </LinearLayout>
        </androidx.appcompat.widget.Toolbar>
