<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="forgotPasswordPageVM" type="com.blanksapplication.app.modules.forgotpasswordpage.data.viewmodel.ForgotPasswordPageVM"/>
    </data>
    <FrameLayout android:id="@+id/frameStackrectanglefour"
        android:layout_height="match_parent"
        android:layout_width="match_parent" style="@style/groupStylewhite_A700">
        <ScrollView android:clipToPadding="false"
            android:fillViewport="true"
            android:id="@+id/scrollViewScrollview"
            android:layout_gravity="start" android:layout_height="0dp"
            android:layout_weight="1" android:layout_width="match_parent">
            <FrameLayout android:id="@+id/frameFORGOTPASSWORD"
                android:layout_height="match_parent" android:layout_width="match_parent">
                <ImageView android:id="@+id/imageRectangleFour"
                    android:layout_gravity="top|start"
                    android:layout_height="@dimen/_238pxv"
                    android:layout_marginBottom="@dimen/_10pxv"
                    android:layout_width="match_parent"
                    android:scaleType="fitXY"
                    imageRes="@{@drawable/img_rectangle4}"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_rectangle4"/>
                <LinearLayout android:id="@+id/linearColumnarrowleft"
                    android:layout_gravity="bottom|start"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_10pxv"
                    android:layout_width="match_parent"
                    android:orientation="vertical" style="@style/groupStylewhite_A700cornerRadius">
                    <LinearLayout android:gravity="start"
                        android:id="@+id/linearRowarrowleft"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_29pxh"
                        android:layout_marginStart="@dimen/_29pxh"
                        android:layout_marginTop="@dimen/_52pxv"
                        android:layout_width="match_parent" android:orientation="horizontal">
                        <ImageView android:id="@+id/imageArrowleft"
                            android:layout_height="@dimen/_48pxh"
                            android:layout_marginBottom="@dimen/_1pxv"
                            android:layout_width="@dimen/_25pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_arrowleft"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_arrowleft"/>
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtForgotPassword"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="@dimen/_24pxh"
                            android:layout_marginTop="@dimen/_2pxv"
                            android:layout_weight="0.91"
                            android:layout_width="0dp"
                            android:singleLine="true"
                            android:text="@{forgotPasswordPageVM.forgotPasswordPageModel.txtForgotPassword}"
                            style="@style/txtIstokWebbold32" tools:text="@string/lbl_forgot_password"/>
                    </LinearLayout>
                    <TextView android:gravity="end"
                        android:id="@+id/txtNoworrieswe"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_57pxh"
                        android:layout_marginStart="@dimen/_57pxh"
                        android:layout_marginTop="@dimen/_15pxv"
                        android:layout_width="match_parent"
                        android:text="@{forgotPasswordPageVM.forgotPasswordPageModel.txtNoworrieswe}"
                        style="@style/txtIstokWebregular20" tools:text="@string/msg_no_worries_we"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtEmail"
                        android:layout_gravity="start"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_60pxh"
                        android:layout_marginStart="@dimen/_60pxh"
                        android:layout_marginTop="@dimen/_21pxv"
                        android:layout_width="wrap_content"
                        android:singleLine="true"
                        android:text="@{forgotPasswordPageVM.forgotPasswordPageModel.txtEmail}"
                        style="@style/txtIstokWebbold20" tools:text="@string/lbl_email"/>
                    <FrameLayout android:id="@+id/frameStackemailOne"
                        android:layout_gravity="end"
                        android:layout_height="@dimen/_70pxv"
                        android:layout_marginEnd="@dimen/_20pxh"
                        android:layout_marginStart="@dimen/_20pxh"
                        android:layout_marginTop="@dimen/_4pxv" android:layout_width="@dimen/_364pxh">
                        <TextView android:ellipsize="end"
                            android:gravity="top|start"
                            android:id="@+id/txtEmailOne"
                            android:layout_gravity="start"
                            android:layout_height="wrap_content"
                            android:layout_marginBottom="@dimen/_16pxv"
                            android:layout_marginEnd="@dimen/_22pxh"
                            android:layout_marginStart="@dimen/_22pxh"
                            android:layout_marginTop="@dimen/_16pxv"
                            android:layout_width="match_parent"
                            android:singleLine="true"
                            android:text="@{forgotPasswordPageVM.forgotPasswordPageModel.txtEmailOne}"
                            style="@style/txtIstokWebbold24" tools:text="@string/msg_someone_example"/>
                        <ImageView android:id="@+id/imageVector"
                            android:layout_gravity="start"
                            android:layout_height="@dimen/_70pxh"
                            android:layout_width="match_parent"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_vector_blue_100"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_vector_blue_100"/>
                    </FrameLayout>
                    <TextView android:ellipsize="end"
                        android:gravity="end"
                        android:id="@+id/txtResetPassword"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_20pxv"
                        android:layout_marginEnd="@dimen/_37pxh"
                        android:layout_marginStart="@dimen/_37pxh"
                        android:layout_marginTop="@dimen/_46pxv"
                        android:layout_width="match_parent"
                        android:paddingBottom="@dimen/_8pxv"
                        android:paddingStart="@dimen/_30pxh"
                        android:paddingTop="@dimen/_7pxv"
                        android:singleLine="true"
                        android:text="@{forgotPasswordPageVM.forgotPasswordPageModel.txtResetPassword}"
                        style="@style/txtSolidRounded" tools:text="@string/lbl_reset_password"/>
                </LinearLayout>
            </FrameLayout>
        </ScrollView>
    </FrameLayout>
</layout>
