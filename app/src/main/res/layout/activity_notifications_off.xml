<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="notificationsOffVM" type="com.blanksapplication.app.modules.notificationsoff.data.viewmodel.NotificationsOffVM"/>
    </data>
    <FrameLayout android:id="@+id/frameStackrectanglefour"
        android:layout_height="match_parent"
        android:layout_width="match_parent" style="@style/groupStylewhite_A700">
        <ScrollView android:clipToPadding="false"
            android:fillViewport="true"
            android:id="@+id/scrollViewScrollview"
            android:layout_gravity="start" android:layout_height="0dp"
            android:layout_weight="1" android:layout_width="match_parent">
            <FrameLayout android:id="@+id/frameNOTIFICATIONSO"
                android:layout_height="match_parent" android:layout_width="match_parent">
                <ImageView android:id="@+id/imageRectangleFour"
                    android:layout_gravity="top|start"
                    android:layout_height="@dimen/_238pxv"
                    android:layout_marginBottom="@dimen/_10pxv"
                    android:layout_width="match_parent"
                    android:scaleType="fitXY"
                    imageRes="@{@drawable/img_rectangle4_238x430}"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_rectangle4_238x430"/>

                <LinearLayout
                    android:id="@+id/linearColumnarrowleft"
                    style="@style/groupStylewhite_A700cornerRadius"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_gravity="bottom|start"
                    android:layout_marginTop="@dimen/_10pxv"
                    android:orientation="vertical">

                    <LinearLayout
                        android:id="@+id/linearRowarrowleft"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_29pxh"
                        android:layout_marginTop="@dimen/_34pxv"
                        android:layout_marginEnd="@dimen/_29pxh"
                        android:gravity="start"
                        android:orientation="horizontal">

                        <ImageView
                            android:id="@+id/imageArrowleft"
                            android:layout_width="@dimen/_25pxh"
                            android:layout_height="@dimen/_48pxh"
                            android:layout_marginTop="@dimen/_1pxv"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_arrowleft"
                            tools:ignore="ContentDescription"
                            tools:src="@drawable/img_arrowleft" />

                        <TextView
                            android:id="@+id/txtNotifications"
                            style="@style/txtIstokWebbold32"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="@dimen/_79pxh"
                            android:layout_marginBottom="@dimen/_1pxv"
                            android:layout_weight="0.88"
                            android:ellipsize="end"
                            android:singleLine="true"
                            android:text="@{notificationsOffVM.notificationsOffModel.txtNotifications}"
                            tools:text="@string/lbl_notifications" />
                    </LinearLayout>

                    <LinearLayout
                        android:id="@+id/linearNotifications"
                        style="@style/groupStyle_1"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_8pxh"
                        android:layout_marginTop="@dimen/_44pxv"
                        android:layout_marginEnd="@dimen/_12pxh"
                        android:layout_marginBottom="@dimen/_633pxv"
                        android:orientation="horizontal">

                        <TextView
                            android:id="@+id/txtDisplaynotific"
                            style="@style/txtIstokWebbold24_2"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="@dimen/_4pxh"
                            android:layout_marginTop="@dimen/_40pxv"
                            android:layout_marginBottom="@dimen/_4pxv"
                            android:layout_weight="0.81"
                            android:ellipsize="end"
                            android:singleLine="true"
                            android:text="@{notificationsOffVM.notificationsOffModel.txtDisplaynotific}"
                            tools:text="@string/msg_display_notific" />

                        <ImageView
                            android:id="@+id/imageUser"
                            android:layout_width="@dimen/_52pxh"
                            android:layout_height="@dimen/_26pxh"
                            android:layout_marginStart="@dimen/_109pxh"
                            android:layout_marginTop="@dimen/_47pxv"
                            android:layout_marginEnd="@dimen/_13pxh"
                            android:layout_marginBottom="@dimen/_5pxv"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_user"
                            tools:ignore="ContentDescription"
                            tools:src="@drawable/img_user" />
                    </LinearLayout>
                </LinearLayout>
            </FrameLayout>
        </ScrollView>
    </FrameLayout>
</layout>
